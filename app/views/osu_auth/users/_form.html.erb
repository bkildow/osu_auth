<%= form_for(@user) do |f| %>
  <%= render 'form_errors' %>

  <%= f.input :first_name, label: 'First Name' %>
  <%= f.input :last_name, label: 'Last Name' %>
  <%= f.input :email, label: 'Email' %>
  <%= f.input :name_n, label: 'Name.#' %>

  <div class="actions">
    <%= f.submit class: 'btn btn-primary' %>
    <%= link_to 'Cancel', users_path, class: 'btn btn-default' %>
  </div>
<% end %>
